{
  "expo": {
    "name": "dayloop",
    "slug": "dayloop",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "dayloop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "icon": "./assets/icon.png",

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codeamus.dayloop",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2b3e4a"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "web": {
      "output": "static",
      "favicon": "./assets/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#2b3e4a"
        }
      ],
      "expo-notifications",
      "expo-font"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "e6d4f38d-e7bc-4b5d-a1db-f12faee449fb"
      }
    }
  }
}
