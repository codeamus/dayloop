{
  "expo": {
    "name": "Dayloop",
    "slug": "dayloop",
    "version": "1.1.0",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "orientation": "portrait",
    "scheme": "dayloop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/icons/adaptive-icon.png",
    "notification": {
      "icon": "./assets/icons/adaptive-icon.png",
      "color": "#2b3e4a"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.codeamus.dayloop",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCalendarsUsageDescription": "Dayloop utiliza el calendario para ayudarte a visualizar tus bloques de tiempo y hábitos diarios."
      },
      "icon": {
        "dark": "./assets/icons/ios-dark.png",
        "light": "./assets/icons/ios-light.png",
        "tinted": "./assets/icons/ios-tinted.png"
      },
      "buildNumber": "111"
    },
    "android": {
      "versionCode": 110,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "backgroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#2b3e4a"
      },
      "notification": {
        "icon": "./assets/icons/adaptive-icon.png",
        "color": "#2b3e4a"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.codeamus.dayloop"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2b3e4a",
          "dark": {
            "image": "./assets/icons/splash-icon-light.png",
            "backgroundColor": "#2b3e4a"
          }
        }
      ],
      "expo-notifications",
      "expo-font",
      [
        "expo-calendar",
        {
          "calendarPermission": "Dayloop utiliza el calendario para ayudarte a visualizar tus bloques de tiempo y hábitos diarios."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9bfb370d-15bf-43f9-86d8-f450b4090343"
      }
    },
    "owner": "codeamus"
  }
}
